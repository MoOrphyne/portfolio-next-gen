<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>

<% @projects.each do |project| %>
  <%= image_tag project.project_image %>
  <h2><%= project.title %></h2>
  <h5><%= project.job_position %></h5>
  <p><%= project.description %></p>
  <p><%= project.date %></p>
  <p><%= project.project_link %></p>
<% end %>
