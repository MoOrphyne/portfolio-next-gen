<h1>Skills#index</h1>
<p>Find me in app/views/skills/index.html.erb</p>

<h2>UI-UX</h2>
<% @skills.each do |skill| %>
  <% if skill.skill_category == "UI-UX" %>
    <%= image_tag skill.skill_image %>
    <h5><%= skill.name %></h2>
  <% end %>
<% end %>

<h2>Dev</h2>
<% @skills.each do |skill| %>
  <% if skill.skill_category == "Dev" %>
    <%= image_tag skill.skill_image %>
    <h5><%= skill.name %></h2>
  <% end %>
<% end %>
